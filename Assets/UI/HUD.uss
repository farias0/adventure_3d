.container {
    flex-grow: 1;
}

.panel {
    flex-grow: 1;
    flex-direction: row;
    margin-top: 30px;
    margin-right: 30px;
    margin-bottom: 30px;
    margin-left: 30px;
}

.health-bar-container {
